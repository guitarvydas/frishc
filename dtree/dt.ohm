dt {
  Main = YesNo
  YesNo = "[" text YesBranch NoBranch "]" -- expr

  YesBranch = "|" "yes" ":" (YesNo | text)
  NoBranch = "|" "no" ":" (YesNo | text)

  text = char+
  
  char =
    | "\n" -- newline
    | "&lt;div&gt;" -- begindiv
    | "&lt;/div&gt;" -- enddiv
    | "&lt;span" (~"&gt;" any)* "&gt;" -- beginspan
    | "&lt;/span&gt;" -- endspan
    | alnum -- alnum
    | (~space ~"[" ~"]" ~"|" ~"yes" ~"no" ~":" ~"&" ~";" any) -- other
}
